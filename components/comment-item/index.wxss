/* components/commitItem/index.wxss */
.commit-item-container{
  display: flex;
  margin: 16rpx 30rpx;
  color: #666;  
}
.cic-lf{
  width: 80rpx;
  height: 80rpx;
  border-radius: 50%;
  margin:0 16rpx 0 0;
}
.cic-ri{
  flex: 1;
  display: flex;
  flex-direction: column;
}
.cic-ri-tp{
  display: flex;
  align-items: baseline;
}
.cic-ri-tp .nickName{
  font-size: 36rpx;  
  color: #000;    
}
.cic-ri-tp .time{
  font-size: 24rpx;
  padding: 0 16rpx;
}